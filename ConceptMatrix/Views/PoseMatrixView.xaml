<UserControl x:Class="ConceptMatrix.Views.PoseMatrixView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:resx="clr-namespace:ConceptMatrix.Resx"
             xmlns:local="clr-namespace:ConceptMatrix.Views" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
             xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" xmlns:Converters="clr-namespace:ConceptMatrix.Converters"
             mc:Ignorable="d" 
             TextElement.FontSize="11.25"
             TextElement.Foreground="{DynamicResource MaterialDesignBody}"
             TextElement.FontWeight="Medium"
             d:DesignHeight="530" d:DesignWidth="1115" Height="530">
    <UserControl.Resources>
        <Converters:QuaternionConverter x:Key="QuaternionConverter" />
        <Style x:Name="HeaderTabs" x:Key="MenuLevel2" BasedOn="{StaticResource MetroTabItem}" TargetType="{x:Type TabItem}">
            <Setter Property="mah:ControlsHelper.HeaderFontSize" Value="16.5"></Setter>
        </Style>
    </UserControl.Resources>
    <Grid Height="530" Width="1119">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="134*"/>
            <ColumnDefinition Width="985*"/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush x:Name="PoseBG" ImageSource="/ConceptMatrix;component/Resources/posebg.png"/>
        </Grid.Background>
        <StackPanel Grid.ColumnSpan="2">
            <StackPanel Orientation="Horizontal">
                <GroupBox Header="{Binding Source={x:Static resx:UIPoseStrings.PoseMatrixSettings}}" Name="PoseMatrixSetting" Width="450" Height="155" Padding="9,0,9,2" IsEnabled="False">
                    <StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Label Content="{Binding Source={x:Static resx:UIPoseStrings.EnableEditing}}"  VerticalAlignment="Center" Width="95"/>
                            <ToggleButton x:Name="EditModeButton" Checked="EditModeButton_Checked" Unchecked="EditModeButton_Unchecked"></ToggleButton>
                            <Label Content="{Binding Source={x:Static resx:UIPoseStrings.ParentRotation}}" VerticalAlignment="Center"/>
                            <ToggleButton IsChecked="{Binding ParentingToggle}"/>
                            <Label Content="{Binding Source={x:Static resx:UIPoseStrings.ToggleHelmParenting}}"   VerticalAlignment="Center" Width="130"/>
                            <ToggleButton x:Name="HelmToggle" IsEnabled="False" Checked="HelmToggle_Checked" Unchecked="HelmToggle_Unchecked" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,2,0,0">
                            <Label Content="Edit Scaling:" Width="95"  VerticalAlignment="Center"/>
                            <ToggleButton x:Name="ScaleEdit" IsEnabled="False" Checked="ScaleEdit_Checked" Unchecked="ScaleEdit_Unchecked"  />
                            <Label Content="Scaling Mode:"  VerticalAlignment="Center" Width="95"/>
                            <ToggleButton x:Name="ScaleToggle" IsEnabled="False" Checked="ScaleToggle_Checked" Unchecked="ScaleToggle_Unchecked" />
                            <Label Content="Weapon Postion Mode:"  VerticalAlignment="Center"/>
                            <ToggleButton x:Name="WeaponPoSToggle" IsEnabled="False" Checked="WeaponPoSToggle_Checked" Unchecked="WeaponPoSToggle_Unchecked"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,2,0,0">
                            <Label Content="{Binding Source={x:Static resx:UIPoseStrings.EnablePhysics}}"  Width="95" VerticalAlignment="Center"/>
                            <ToggleButton Name="PhysicsButton" Checked="PhysicsButton_Checked" Unchecked="PhysicsButton_Unchecked" IsEnabled="False" />
                            <Label Content="Scaling In Load:" Width="95" />
                            <ToggleButton x:Name="ScaleSaveToggle" Checked="ScaleSaveToggle_Checked" Unchecked="ScaleSaveToggle_Unchecked"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,3,0,0">
                            <Button x:Name="SaveCMP" Width="156" Margin="25,0,0,0" Click="SaveCMP_Click">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="ContentSave" Padding="0" Margin="0,2,0,0" />
                                    <TextBlock Margin="8 0 0 0" VerticalAlignment="Center" Padding="0" Text="{Binding Source={x:Static resx:UIPoseStrings.SaveCMPFile}}" />
                                </StackPanel>
                            </Button>
                            <Button x:Name="LoadCMP" Width="156" Margin="50,0,0,0" Click="LoadCMP_Click">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="Download" Padding="0" Margin="0,2,0,0" />
                                    <TextBlock Margin="8 0 0 0" VerticalAlignment="Center" Padding="0" Text="{Binding Source={x:Static resx:UIPoseStrings.LoadCMPFile}}" />
                                </StackPanel>
                            </Button>
                        </StackPanel>
                    </StackPanel>
                </GroupBox>
                <GroupBox Header="{Binding Source={x:Static resx:UIPoseStrings.AdvancedLoading}}"  Height="155" Padding="9,0,9,2" Margin="10,0,0,0" Width="473">
                    <StackPanel>
                        <StackPanel Orientation="Vertical">
                            <StackPanel Orientation="Horizontal" Margin="-10,5,0,0">
                                <CheckBox x:Name="HeadAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.Head2}}" Margin="10,0,0,0" VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="BodyAdvLoad" Margin="10,0,0,0" Content="{Binding Source={x:Static resx:UIPoseStrings.Body2}}"  VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="ClothesAdvLoad" Margin="10,0,0,0" Content="{Binding Source={x:Static resx:UIPoseStrings.Clothes2}}"  VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="WeaponsAdvLoad" Margin="10,0,0,0" Content="{Binding Source={x:Static resx:UIPoseStrings.Weapons3}}"  VerticalAlignment="Center" Width="90" Padding="0"/>
                                <CheckBox x:Name="WaistAdvLoad" Margin="10,0,0,0" Content="{Binding Source={x:Static resx:UIPoseStrings.Waist2}}"  VerticalAlignment="Center" Width="90" Padding="0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="-10,5,0,0">
                                <CheckBox x:Name="HairAdvLoad"  Content="{Binding Source={x:Static resx:UIPoseStrings.Hair2}}"  Margin="10,0,0,0" VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="LeftArmAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.LeftArm2}}" Margin="10,0,0,0"  VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="HelmAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.Helm2}}"  Margin="10,0,0,0" VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="LeftHandAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.LeftHand2}}" Margin="10,0,0,0"  VerticalAlignment="Center" Width="90" Padding="0"/>
                                <CheckBox x:Name="LeftLegAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.LeftLeg2}}"  Margin="10,0,0,0" VerticalAlignment="Center" Width="90" Padding="0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="-10,5,0,0">
                                <CheckBox x:Name="EarringsAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.Earrings2}}"  Margin="10,0,0,0" VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="RightArmAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.RightArm2}}" Margin="10,0,0,0" VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="TopAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.Top2}}" Margin="10,0,0,0" VerticalAlignment="Center" Width="80" Padding="0"/>
                                <CheckBox x:Name="RightHandAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.RightHand2}}" Margin="10,0,0,0" VerticalAlignment="Center" Width="90" Padding="0"/>
                                <CheckBox x:Name="RightLegAdvLoad" Content="{Binding Source={x:Static resx:UIPoseStrings.RightLeg2}}" Margin="10,0,0,0" VerticalAlignment="Center" Width="90" Padding="0"/>
                            </StackPanel>

                            <Button x:Name="AdvLoadCMP" Width="200" Margin="0,10,0,0" Click="AdvLoadCMP_Click">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="Download" Padding="0" Margin="0,2,0,0" />
                                    <TextBlock Margin="8 0 0 0" VerticalAlignment="Center" Padding="0" Text="{Binding Source={x:Static resx:UIPoseStrings.AdvLoadCMPFile}}" />
                                </StackPanel>
                            </Button>
                        </StackPanel>
                    </StackPanel>
                </GroupBox>
                <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="10,40,0,0">
                    <Button x:Name="SelectAll" Content="{Binding Source={x:Static resx:UIPoseStrings.SelectAll}}" Width="156" Click="SelectAll_Click" />
                    <Button x:Name="SelectNone" Content="{Binding Source={x:Static resx:UIPoseStrings.SelectNone}}" Width="156" Margin="0,20,0,0" Click="SelectNone_Click"/>
                </StackPanel>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0,5,0,0">
                <Label Content="{Binding Source={x:Static resx:UIPoseStrings.XRot}}"  VerticalAlignment="Center"/>
                <mah:NumericUpDown x:Name="XUpDown" Width="125" Value="{Binding UserBoneX, Mode=TwoWay, NotifyOnSourceUpdated=True}" Background="{x:Null}" TextAlignment="Right"  BorderThickness="2" Maximum="360" Minimum="0" StringFormat="F5" Interval="{Binding BoneInterval}" FontSize="11.35" FontFamily="Segoe UI"/>
                <Slider x:Name="BoneSliderX" Value="{Binding UserBoneX, Mode=TwoWay, NotifyOnSourceUpdated=True}"
                                Margin="5,5,0,0" Width="165" Minimum="0" Maximum="360" TickFrequency="0.5" />
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="{Binding Source={x:Static resx:UIPoseStrings.YRot}}"  VerticalAlignment="Center"/>
                <mah:NumericUpDown x:Name="YUpDown" Width="125" Value="{Binding UserBoneY, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}"  Background="{x:Null}" TextAlignment="Right"  BorderThickness="2" Maximum="360" Minimum="0" StringFormat="F5" Interval="{Binding BoneInterval}" FontSize="11.35" FontFamily="Segoe UI"/>
                <Slider x:Name="BoneSliderY" Value="{Binding UserBoneY, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}"
                                Margin="5,5,0,0" Width="165" Minimum="0" Maximum="360" TickFrequency="0.5" />
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="{Binding Source={x:Static resx:UIPoseStrings.ZRot}}"  VerticalAlignment="Center"/>
                <mah:NumericUpDown x:Name="ZUpDown" Width="125" Value="{Binding UserBoneZ, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Background="{x:Null}" TextAlignment="Right"  BorderThickness="2" Maximum="360" Minimum="0"  StringFormat="F5" Interval="{Binding BoneInterval}" FontSize="11.35" FontFamily="Segoe UI"/>
                <Slider x:Name="BoneSliderZ" Value="{Binding UserBoneZ, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}"
                                Margin="5,5,0,0" Width="165" Minimum="0" Maximum="360" TickFrequency="0.5"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                <GroupBox Header="Cube Rotation - Bones" x:Name="CubeBox">
                    <Border BorderBrush="Black" BorderThickness="3" Height="155" Width="200" Background="#FF2E2E2E">
                        <local:PoseRotationView x:Name="Cube" IsEnabled="False">
                            <local:PoseRotationView.DataContext>
                                <MultiBinding Converter="{StaticResource ResourceKey=QuaternionConverter}">
                                    <Binding Path="CubeBone_X" NotifyOnSourceUpdated="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" />
                                    <Binding Path="CubeBone_Y" NotifyOnSourceUpdated="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" />
                                    <Binding Path="CubeBone_Z" NotifyOnSourceUpdated="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" />
                                    <Binding Path="CubeBone_W" NotifyOnSourceUpdated="True" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay" />
                                </MultiBinding>
                            </local:PoseRotationView.DataContext>
                        </local:PoseRotationView>
                    </Border>
                </GroupBox>
            </StackPanel>
        </StackPanel>
        <GroupBox Margin="0,250,754,60" Grid.ColumnSpan="2" Header="Actor Model Properties" x:Name="ActorProperties" Visibility="Hidden">
            <StackPanel>
                <StackPanel Orientation="Horizontal">
                    <!-- X Rot -->
                    <CheckBox IsChecked="{Binding CharacterDetails.CharacterDetails.RotateFreeze}" Content="{Binding Source={x:Static resx:UIStringsContent.XRot}}"  x:Name="RotateCheckBox1" Height="28"/>
                    <mah:NumericUpDown Minimum="0" Maximum="360" x:Name="RotationUpDown" SourceUpdated="RotationUpDown_SourceUpdated" Value="{Binding CharacterDetails.CharacterDetails.RotateX, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="150" Background="{x:Null}" BorderThickness="0,0,0,1" BorderBrush="#8C0000FF" StringFormat="F5" Interval="0.5" TextAlignment="Right" Height="28" MinWidth="0" MinHeight="0" FontSize="11.35" FontFamily="Segoe UI"/>
                    <Slider Name="RotationSlider" SourceUpdated="RotationUpDown_SourceUpdated" Value="{Binding CharacterDetails.CharacterDetails.RotateX, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="115" Minimum="0" Maximum="360" TickFrequency="0.5"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <!-- Y Rot -->
                    <CheckBox IsChecked="{Binding CharacterDetails.CharacterDetails.RotateFreeze}" Content="{Binding Source={x:Static resx:UIStringsContent.YRot}}"  x:Name="RotateCheckBox2" Height="28"/>
                    <mah:NumericUpDown Minimum="0" Maximum="360" x:Name="RotationUpDown2" SourceUpdated="RotationUpDown2_SourceUpdated" Value="{Binding CharacterDetails.CharacterDetails.RotateY, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="150" Background="{x:Null}" BorderThickness="0,0,0,1" BorderBrush="#8C008000" StringFormat="F5" Interval="0.5" TextAlignment="Right" Height="28" MinWidth="0" MinHeight="0" FontSize="11.35" FontFamily="Segoe UI"/>
                    <Slider Name="RotationSlider2" SourceUpdated="RotationUpDown2_SourceUpdated" Value="{Binding CharacterDetails.CharacterDetails.RotateY, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="115" Minimum="0" Maximum="360" TickFrequency="0.5"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <!-- Z Rot -->
                    <CheckBox IsChecked="{Binding CharacterDetails.CharacterDetails.RotateFreeze}" Content="{Binding Source={x:Static resx:UIStringsContent.ZRot}}" x:Name="RotateCheckBox3" Height="28"/>
                    <mah:NumericUpDown Minimum="0" Maximum="360" x:Name="RotationUpDown3" SourceUpdated="RotationUpDown3_SourceUpdated" Value="{Binding CharacterDetails.CharacterDetails.RotateZ, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="150" Background="{x:Null}" BorderThickness="0,0,0,1" BorderBrush="#8CFF0000" StringFormat="F5" Interval="0.5" TextAlignment="Right" Height="28" MinWidth="0" MinHeight="0" FontSize="11.35" FontFamily="Segoe UI"/>
                    <Slider Name="RotationSlider3" SourceUpdated="RotationUpDown3_SourceUpdated" Value="{Binding CharacterDetails.CharacterDetails.RotateZ, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="115" Minimum="0" Maximum="360" TickFrequency="0.5"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <!-- X Pos -->
                    <CheckBox IsChecked="{Binding CharacterDetails.CharacterDetails.X.freeze}" x:Name="XCheck" Content="{Binding Source={x:Static resx:UIStringsContent.XPos}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.XPosToolTip}}" Height="28"/>
                    <mah:NumericUpDown x:Name="PosX" Value="{Binding CharacterDetails.CharacterDetails.X.value, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="150" Background="{x:Null}" BorderThickness="0,0,0,1" BorderBrush="#8C0000FF" StringFormat="F5" Interval="0.01" SourceUpdated="PosX_SourceUpdated" TextAlignment="Right" MinWidth="0" MinHeight="0" Height="28" FontSize="11.35" FontFamily="Segoe UI"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <!-- Y Pos -->
                    <CheckBox IsChecked="{Binding CharacterDetails.CharacterDetails.Y.freeze}" x:Name="YCheck" Content="{Binding Source={x:Static resx:UIStringsContent.YPos}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.YPosToolTip}}" Height="28"/>
                    <mah:NumericUpDown x:Name="PosY" Value="{Binding CharacterDetails.CharacterDetails.Y.value, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="150" Background="{x:Null}" BorderThickness="0,0,0,1" BorderBrush="#8C008000" StringFormat="F5" Interval="0.01" SourceUpdated="PosY_SourceUpdated" TextAlignment="Right" MinWidth="0" MinHeight="0" Height="28" FontSize="11.35" FontFamily="Segoe UI"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <!-- Z Post -->
                    <CheckBox IsChecked="{Binding CharacterDetails.CharacterDetails.Z.freeze}" x:Name="ZCheck" Content="{Binding Source={x:Static resx:UIStringsContent.ZPos}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.ZPosToolTip}}" Height="28"/>
                    <mah:NumericUpDown x:Name="PosZ" Value="{Binding CharacterDetails.CharacterDetails.Z.value, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Width="150" Background="{x:Null}" BorderThickness="0,0,0,1" BorderBrush="#8CFF0000" StringFormat="F5" Interval="0.01" SourceUpdated="PosZ_SourceUpdated" TextAlignment="Right" Height="28" MinWidth="0" MinHeight="0" FontSize="11.35" FontFamily="Segoe UI"/>
                    <!-- Relative Pos -->
                    <Label Content="Relative PoS:" ToolTip="{Binding Source={x:Static resx:UIStringsContent.RelativeXYZPosToolTip}}" Height="28"/>
                    <ToggleButton Name="PosRelButton" Style="{StaticResource MaterialDesignSwitchToggleButton}" IsChecked="{Binding CharacterDetails.CharacterDetails.RelativePositioning, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Height="28" BorderThickness="1" Checked="PosRelButton_Checked" Unchecked="PosRelButton_Unchecked" />
                </StackPanel>
            </StackPanel>

        </GroupBox>
        <CheckBox x:Name="ToggleModelView" Content="Toggle Actor/Model Box" Grid.ColumnSpan="2" Margin="10,460,949,35" Checked="ToggleModelView_Checked" Unchecked="ToggleModelView_Unchecked"/>
        <mah:NumericUpDown Value="{Binding BoneInterval, Mode=TwoWay, NotifyOnSourceUpdated=True, UpdateSourceTrigger=PropertyChanged}" Background="{x:Null}" TextAlignment="Right"  BorderThickness="2" Maximum="360" Minimum="0"  StringFormat="F3" Interval="0.01" FontSize="11.35" FontFamily="Segoe UI" Margin="95,496,909,8" Grid.ColumnSpan="2"/>
        <Label Content="Bone Intervals:" Margin="9,496,39,9"/>
        <mah:MetroAnimatedTabControl  Background="{x:Null}" Grid.Column="1" Margin="231,155,10,0">
            <TabItem Header="{Binding Source={x:Static resx:UIPoseStrings.HeadHair}}" Style="{DynamicResource MenuLevel2}">
                <StackPanel>
                    <GroupBox Header="{Binding Source={x:Static resx:UIPoseStrings.Head2}}" Padding="0,0,0,5" Width="730">
                        <StackPanel>
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Face}}"  VerticalAlignment="Center" Width="60"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Name="Head" Checked="Toggles_Checked"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Ears}}"  VerticalAlignment="Center" Width="auto"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EarLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EarRight" Content="R"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.VieraEars}}"  VerticalAlignment="Center" Width="80"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="VieraEarALeft" Content="L1"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="VieraEarBLeft" Content="L2"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked"  Name="VieraEarARight" Content="R1"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="VieraEarBRight" Content="R2"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Nose}}"  VerticalAlignment="Center" Width="60"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="Nose"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Bridge}}"   VerticalAlignment="Center" Width="auto"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="Bridge"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Eyes}}"  VerticalAlignment="Center" Width="60"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyeLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyeRight" Content="R"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Eyebrows}}"  VerticalAlignment="Center" Width="auto"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyebrowLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyebrowRight" Content="R"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Brows}}"  VerticalAlignment="Center" Width="auto"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="BrowLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="BrowRight" Content="R"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Eyelids}}"  VerticalAlignment="Center" Width="auto"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyelidUpperLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyelidUpperRight" Content="R"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.LowerLids}}"  VerticalAlignment="Center" Width="auto"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyelidLowerLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="EyelidLowerRight" Content="R"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Cheeks}}"  VerticalAlignment="Center" Width="60"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="CheekLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="CheekRight" Content="R"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Jaw}}"  VerticalAlignment="Center" Width="60"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="Jaw"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Mouth}}"  VerticalAlignment="Center" Margin="34,0,0,0" Width="60"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="LipsLeft" Content="L"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="LipsRight" Content="R"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.UpperLip}}" Width="auto"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="LipUpperA" Content="L1"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="LipUpperB" Content="R1"/>
                                    <Label Content="{Binding Source={x:Static resx:UIPoseStrings.LowerLip}}" Width="auto" />
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="LipLowerA" Content="L2"/>
                                    <ToggleButton Unchecked="Toggles_Unchecked" IsEnabled="False" Checked="Toggles_Checked" Name="LipLowerB"  Content="R2"/>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>

                    </GroupBox>
                    <StackPanel Orientation="Horizontal" Margin="0,5,0,0">
                        <Label Content="{Binding Source={x:Static resx:UIPoseStrings.Head}}"  VerticalAlignment="Center" Width="75"/>
                        <Button x:Name="SavestateHead01" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50" Height="28" Padding="0" Click="SavestateHead01_Click">
                            <StackPanel Orientation="Horizontal" Width="50" Height="16">
                                <materialDesign:PackIcon Kind="ContentSave" Margin="12 0 0 0" Padding="0" Width="16" Height="16" />
                                <TextBlock Margin="2 -1 0 0" VerticalAlignment="Center" Padding="0" Text="1" FontSize="16" />
                            </StackPanel>
