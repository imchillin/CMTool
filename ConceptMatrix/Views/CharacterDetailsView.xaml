<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Views="clr-namespace:ConceptMatrix.Views"
             xmlns:Converters="clr-namespace:ConceptMatrix.Converters"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:resx="clr-namespace:ConceptMatrix.Resx"
             x:Class="ConceptMatrix.Views.CharacterDetailsView"
             TextElement.Foreground="{DynamicResource MaterialDesignBody}"
             TextElement.FontWeight="Medium"
             TextElement.FontSize="13"
             mc:Ignorable="d" d:DesignWidth="1115" d:DesignHeight="460" Height="530">

	<UserControl.Resources>
		<Converters:QuaternionConverter x:Key="QuaternionConverter" />
	</UserControl.Resources>
	<UserControl.Background>
		<ImageBrush x:Name="ActorBG" ImageSource="/ConceptMatrix;component/Resources/actorbg.png"/>
	</UserControl.Background>

	<Grid>
		<Border BorderBrush="Black" BorderThickness="0,0,1,0" HorizontalAlignment="Left" Height="530" Margin="-110,0,0,0" VerticalAlignment="Top" Width="645"/>
		<Border BorderBrush="Black" BorderThickness="1,1,1,0" HorizontalAlignment="Left" Height="190" Margin="534,340,-1,0" VerticalAlignment="Top" Width="582" Grid.ColumnSpan="2"/>

		<!-- FC Tag -->
		<CheckBox  IsChecked="{Binding CharacterDetails.FCTag.freeze}" IsEnabled="False" Content="{Binding Source={x:Static resx:UIStringsContent.FreeCompanyTag}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.FreeCompanyToolTip}}" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" IsEnabled="False" x:Name="FCTagBox" HorizontalAlignment="Left" Height="23" Margin="168,35,0,0" Text="{Binding CharacterDetails.FCTag.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="128"  materialDesign:HintAssist.Hint="{Binding Source={x:Static resx:UIStringsContent.FreeCompanyTagWaterMark}}" FontSize="11.35" MaxLength="6" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Job Icon -->
		<CheckBox IsChecked="{Binding CharacterDetails.JobIco.freeze}" IsEnabled="False" Content="{Binding Source={x:Static resx:UIStringsContent.JobIcon}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.JobIconToolTip}}" HorizontalAlignment="Left" Margin="311,35,0,0" VerticalAlignment="Top" Height="23"/>
		<ComboBox SelectedIndex="{Binding CharacterDetails.JobIco.value}" IsEnabled="False" HorizontalAlignment="Left" Margin="396,35,0,0" VerticalAlignment="Top" Width="128" Height="23" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Padding="0,1">
			<ComboBoxItem Content="None" Background="{x:Null}"/>
			<ComboBoxItem Content="Gladiator" Background="{x:Null}"/>
			<ComboBoxItem Content="Pugilist" Background="{x:Null}"/>
			<ComboBoxItem Content="Marauder" Background="{x:Null}"/>
			<ComboBoxItem Content="Lancer" Background="{x:Null}"/>
			<ComboBoxItem Content="Archer" Background="{x:Null}"/>
			<ComboBoxItem Content="Conjurer" Background="{x:Null}"/>
			<ComboBoxItem Content="Thaumaturge" Background="{x:Null}"/>
			<ComboBoxItem Content="Carpenter" Background="{x:Null}"/>
			<ComboBoxItem Content="Blacksmith" Background="{x:Null}"/>
			<ComboBoxItem Content="Armorer" Background="{x:Null}"/>
			<ComboBoxItem Content="Goldsmith" Background="{x:Null}"/>
			<ComboBoxItem Content="Leatherworker" Background="{x:Null}"/>
			<ComboBoxItem Content="Weaver" Background="{x:Null}"/>
			<ComboBoxItem Content="Alchemist" Background="{x:Null}"/>
			<ComboBoxItem Content="Culinarian" Background="{x:Null}"/>
			<ComboBoxItem Content="Miner" Background="{x:Null}"/>
			<ComboBoxItem Content="Botanist" Background="{x:Null}"/>
			<ComboBoxItem Content="Fisher" Background="{x:Null}"/>
			<ComboBoxItem Content="Paladin" Background="{x:Null}"/>
			<ComboBoxItem Content="Monk" Background="{x:Null}"/>
			<ComboBoxItem Content="Warrior" Background="{x:Null}"/>
			<ComboBoxItem Content="Dragoon" Background="{x:Null}"/>
			<ComboBoxItem Content="Bard" Background="{x:Null}"/>
			<ComboBoxItem Content="White Mage" Background="{x:Null}"/>
			<ComboBoxItem Content="Black Mage" Background="{x:Null}"/>
			<ComboBoxItem Content="Arcanist" Background="{x:Null}"/>
			<ComboBoxItem Content="Summoner" Background="{x:Null}"/>
			<ComboBoxItem Content="Scholar" Background="{x:Null}"/>
			<ComboBoxItem Content="Rogue" Background="{x:Null}"/>
			<ComboBoxItem Content="Ninja" Background="{x:Null}"/>
			<ComboBoxItem Content="Machinist" Background="{x:Null}"/>
			<ComboBoxItem Content="Dark Knight" Background="{x:Null}"/>
			<ComboBoxItem Content="Astrologian" Background="{x:Null}"/>
			<ComboBoxItem Content="Samurai" Background="{x:Null}"/>
			<ComboBoxItem Content="Red Mage" Background="{x:Null}"/>
			<ComboBoxItem Content="Blue Mage" Background="{x:Null}"/>
			<ComboBoxItem Content="Gunbreaker" Background="{x:Null}"/>
			<ComboBoxItem Content="Dancer" Background="{x:Null}"/>
			<ComboBoxItem Content="Chocobo" Background="{x:Null}"/>
			<ComboBoxItem Content="Pet" Background="{x:Null}"/>
			<ComboBoxItem Content="Other Role" Background="{x:Null}"/>
			<ComboBoxItem Content="All Roles" Background="{x:Null}"/>
		</ComboBox>

		<!-- Title -->
		<CheckBox IsChecked="{Binding CharacterDetails.Title.freeze}" IsEnabled="False" Content="{Binding Source={x:Static resx:UIStringsContent.Title}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.TitleToolTip}}" HorizontalAlignment="Left" Margin="311,5,0,0" VerticalAlignment="Top" Height="23"/>
		<ComboBox x:Name="TitleBox" IsEnabled="False" SelectedIndex="{Binding CharacterDetails.Title.value}" HorizontalAlignment="Left" Margin="396,5,0,0" VerticalAlignment="Top" Width="128" Height="23" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Padding="0,1"/>

		<Border BorderBrush="Black" BorderThickness="0,1,0,0" HorizontalAlignment="Left" Height="462" Margin="0,68,0,0" VerticalAlignment="Top" Width="535"/>

		<!-- Name -->
		<CheckBox  IsChecked="{Binding CharacterDetails.Name.freeze}" IsEnabled="False" Content="{Binding Source={x:Static resx:UIStringsContent.Name}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.NameToolTip}}" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" IsEnabled="False" x:Name="NameBox" HorizontalAlignment="Left" Height="23" Margin="168,5,0,0" Text="{Binding CharacterDetails.Name.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="128"  materialDesign:HintAssist.Hint="{Binding Source={x:Static resx:UIStringsContent.NameWaterMark}}" KeyUp="NameBox_KeyUp" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Race -->
		<CheckBox IsChecked="{Binding CharacterDetails.Race.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.Race}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.RaceToolTip}}" HorizontalAlignment="Left" Margin="8,77,0,0" VerticalAlignment="Top" Height="23"/>
		<ComboBox x:Name="RaceBox" SelectedIndex="{Binding CharacterDetails.Race.value}" HorizontalAlignment="Left" Margin="168,77.118,0,0" VerticalAlignment="Top" Width="128" Height="23" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Padding="0,1"/>

		<!-- Clan -->
		<CheckBox IsChecked="{Binding CharacterDetails.Clan.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.Clan}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.ClanToolTip}}" HorizontalAlignment="Left" Margin="8,107,0,0" VerticalAlignment="Top" Height="23"/>
		<ComboBox x:Name="ClanBox" SelectedIndex="{Binding CharacterDetails.Clan.value}" HorizontalAlignment="Left" Margin="168,107.118,0,0" VerticalAlignment="Top" Width="128" Height="23" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Padding="0,1"/>

		<!-- Gender -->
		<CheckBox IsChecked="{Binding CharacterDetails.Gender.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.Gender}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.GenderToolTip}}" HorizontalAlignment="Left" Margin="8,137,0,0" VerticalAlignment="Top" Height="23"/>
		<ComboBox SelectedIndex="{Binding CharacterDetails.Gender.value}" HorizontalAlignment="Left" Margin="168,137.118,0,0" VerticalAlignment="Top" Width="128" Height="23" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Padding="0,1">
			<ComboBoxItem Content="{Binding Source={x:Static resx:MiscStrings.Male}}" Background="{x:Null}"/>
			<ComboBoxItem Content="{Binding Source={x:Static resx:MiscStrings.Female}}" Background="{x:Null}"/>
		</ComboBox>

		<!-- Body Type -->
		<CheckBox IsChecked="{Binding CharacterDetails.BodyType.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.BodyType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.BodyToolTip}}" HorizontalAlignment="Left" Margin="311,137,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" Height="23" Margin="469,137.118,0,0" Text="{Binding CharacterDetails.BodyType.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" PreviewTextInput="DigitCheckInput" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Height -->
		<CheckBox IsChecked="{Binding CharacterDetails.RHeight.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.Height}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.HeightToolTip}}" HorizontalAlignment="Left" Margin="311,77,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" Height="23" Margin="469,77.118,0,0" Text="{Binding CharacterDetails.RHeight.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" PreviewTextInput="DigitCheckInput" FontSize="11.35" CaretBrush="#FF3F51B5" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Bust Size -->
		<CheckBox IsChecked="{Binding CharacterDetails.RBust.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.BustSize}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.BustSizeToolTip}}" HorizontalAlignment="Left" Margin="311,107,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" Height="23"  PreviewTextInput="DigitCheckInput"  Margin="469,107.118,0,0" Text="{Binding CharacterDetails.RBust.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Skintone -->
		<CheckBox IsChecked="{Binding CharacterDetails.Skintone.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.Skintone}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.SkinToneToolTip}}" HorizontalAlignment="Left" Margin="8,167,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" Height="23"  PreviewTextInput="DigitCheckInput" Margin="168,167.118,0,0" Text="{Binding CharacterDetails.Skintone.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="SkinSearch" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,167.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="SkinSearch_Click"/>

		<!-- Head Type -->
		<CheckBox IsChecked="{Binding CharacterDetails.Head.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.HeadType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.HeadTypeToolTip}}" HorizontalAlignment="Left" Margin="311,167,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" Height="23"  PreviewTextInput="DigitCheckInput" Margin="469,167.118,0,0" Text="{Binding CharacterDetails.Head.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Hairstyle -->
		<CheckBox IsChecked="{Binding CharacterDetails.Hair.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.Hairstyle}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.HairStyleToolTip}}" HorizontalAlignment="Left" Margin="8,407,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" Height="23"  PreviewTextInput="DigitCheckInput" Margin="168,407.118,0,0" Text="{Binding CharacterDetails.Hair.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="HairSelectButton" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,407.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="HairSelectButton_Click_1"/>

		<!-- Hair Color -->
		<CheckBox IsChecked="{Binding CharacterDetails.HairTone.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.HairColor}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.HairColorToolTip}}" HorizontalAlignment="Left" Margin="8,437,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left"  PreviewTextInput="DigitCheckInput" Height="23" Margin="168,437.118,0,0" Text="{Binding CharacterDetails.HairTone.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="HairColorSearch"  Content="{Binding Source={x:Static resx:UIStringsContent.View}}" HorizontalAlignment="Left" materialDesign:ShadowAssist.ShadowDepth="Depth3" Margin="231,437.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="HairSelectButton_Click"/>

		<!-- Highlights -->
		<CheckBox Name="HighlightCheckbox" IsChecked="{Binding CharacterDetails.Highlights.freeze}" Content="" HorizontalAlignment="Left" Margin="103,467,0,0" VerticalAlignment="Top" Height="23" Visibility="Hidden" IsEnabled="False"/>
		<ToggleButton x:Name="HighLightButton" ToolTip="{Binding Source={x:Static resx:UIStringsContent.HighLightButtonToolTip}}" Style="{StaticResource MaterialDesignSwitchToggleButton}" IsChecked="{Binding CharacterDetails.Highlights.SpecialActivate}" Margin="128,467,0,0"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Checked="HighLightButton_Checked" Unchecked="HighLightButton_Unchecked" />
		<CheckBox IsChecked="{Binding CharacterDetails.HighlightTone.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.Highlights}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.HighlightsToolTip}}" HorizontalAlignment="Left" Margin="8,467,0,0" VerticalAlignment="Top" Height="23" Checked="HighlightCheckbox_Checked" Unchecked="HighlightCheckbox_Unchecked"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left"  PreviewTextInput="DigitCheckInput" Height="23" Margin="168,467.118,0,0" Text="{Binding CharacterDetails.HighlightTone.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="HighlightcolorSearch" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,467.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="HighlightcolorSearch_Click"/>

		<!-- Mouth Type -->
		<CheckBox IsChecked="{Binding CharacterDetails.Lips.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.MouthType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.MouthTypeToolTip}}" HorizontalAlignment="Left" Margin="311,317,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left"  PreviewTextInput="DigitCheckInput" Height="23" Margin="469,317.118,0,0" Text="{Binding CharacterDetails.Lips.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Lips Color -->
		<CheckBox IsChecked="{Binding CharacterDetails.LipsTone.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.LipColor}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.LipColorToolTip}}" HorizontalAlignment="Left" Margin="8,287,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left"  PreviewTextInput="DigitCheckInput" Height="23" Margin="168,287.118,0,0" Text="{Binding CharacterDetails.LipsTone.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="LipColorSearch" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,287.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="Button_Click"/>

		<!-- Jaw Type -->
		<CheckBox IsChecked="{Binding CharacterDetails.Jaw.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.JawType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.JawTypeToolTip}}" HorizontalAlignment="Left" Margin="311,197,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="469,197.118,0,0" Text="{Binding CharacterDetails.Jaw.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Nose Type -->
		<CheckBox IsChecked="{Binding CharacterDetails.Nose.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.NoseType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.NoseTypeToolTip}}" HorizontalAlignment="Left" Margin="311,287,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="469,287.118,0,0" Text="{Binding CharacterDetails.Nose.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Eyes -->
		<CheckBox IsChecked="{Binding CharacterDetails.Eye.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.EyesType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.EyeTypeToolTip}}" HorizontalAlignment="Left" Margin="311,257,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="469,257.118,0,0" Text="{Binding CharacterDetails.Eye.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Eyebrows Type -->
		<CheckBox IsChecked="{Binding CharacterDetails.EyeBrowType.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.EyebrowsType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.EyeBrowsToolTip}}" HorizontalAlignment="Left" Margin="311,227,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="469,227.118,0,0" Text="{Binding CharacterDetails.EyeBrowType.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Eye Color (Right) -->
		<CheckBox IsChecked="{Binding CharacterDetails.RightEye.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.EyeColorRight}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.EyeColorRightToolTip}}" HorizontalAlignment="Left" Margin="8,227,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="168,227.118,0,0" Text="{Binding CharacterDetails.RightEye.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="RightEyeSearch" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,227.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="RightEyeSearch_Click"/>

		<!-- Eye Color (Left) -->
		<CheckBox IsChecked="{Binding CharacterDetails.LeftEye.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.EyeColorLeft}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.EyeColorLeftToolTip}}" HorizontalAlignment="Left" Margin="8,197,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="168,197.118,0,0" Text="{Binding CharacterDetails.LeftEye.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="LeftEyeSearch" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,197.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="LeftEyeSearch_Click" BorderThickness="1"/>

		<!-- Limbal Eye/Tattoo Color -->
		<CheckBox IsChecked="{Binding CharacterDetails.LimbalEyes.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.LimbalOrTattoo}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.LimbalOrTattooToolTip}}" HorizontalAlignment="Left" Margin="8,377,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="168,377.118,0,0" Text="{Binding CharacterDetails.LimbalEyes.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="LimbalEyeSearch" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,377.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="LimbalEyeSearch_Click"/>

		<!-- Facial Features -->
		<CheckBox IsChecked="{Binding CharacterDetails.FacialFeatures.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.FacialFeatures}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.FacialFeaturesToolTip}}" HorizontalAlignment="Left" Margin="8,257,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="168,257.118,0,0" Text="{Binding CharacterDetails.FacialFeatures.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>
		<Button x:Name="FacialFeature" Content="{Binding Source={x:Static resx:UIStringsContent.View}}" materialDesign:ShadowAssist.ShadowDepth="Depth3" HorizontalAlignment="Left" Margin="231,257.118,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="FacialFeature_Click"/>

		<!-- Ear/Tail Type -->
		<CheckBox IsChecked="{Binding CharacterDetails.TailType.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.EarTailType}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.EarTailTypeToolTip}}" HorizontalAlignment="Left" Margin="311,347,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="469,347.118,0,0" Text="{Binding CharacterDetails.TailType.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Ear/Muscle/Tail Size -->
		<CheckBox IsChecked="{Binding CharacterDetails.TailorMuscle.freeze}" Content="{Binding Source={x:Static resx:UIStringsContent.EarMuscleTailSize}}" ToolTip="{Binding Source={x:Static resx:UIStringsContent.EarMuscleTailSizeToolTip}}" HorizontalAlignment="Left" Margin="311,377,0,0" VerticalAlignment="Top" Height="23"/>
		<TextBox Style="{StaticResource MaterialDesignTextBox}" HorizontalAlignment="Left" PreviewTextInput="DigitCheckInput" Height="23" Margin="469,377.118,0,0" Text="{Binding CharacterDetails.TailorMuscle.value, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top" Width="55" TextAlignment="Center" FontSize="11.35" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"/>

		<!-- Face Paint/Tattoos -->
